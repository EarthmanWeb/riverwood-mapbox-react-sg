{
    "image": "mcr.microsoft.com/devcontainers/javascript-node:18-bullseye",
    "postCreateCommand": "sh .devcontainer/postCreate.sh",
    "customizations": {
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint",
                "esbenp.prettier-vscode",
                "dsznajder.es7-react-js-snippets"
            ]
        }
    },
    "remoteUser": "node",
    "forwardPorts": [3000]
}
